{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://vulnlog.dev/vulnerabilities.analysis.verdict.vulnlog.schema.json",
  "title": "Vulnlog Vulnerability Analysis Verdict Schema",
  "type": "object",
  "properties": {
    "not-affected": {
      "title": "Not Affected",
      "description": "The reported vulnerability does not affects the product.",
      "$ref": "#/$defs/not-affected"
    },
    "affected": {
      "title": "Affected",
      "description": "The reported vulnerability affects the product.",
      "$ref": "#/$defs/affected"
    }
  },
  "$defs": {
    "not-affected": {
      "$ref": "vulnerabilities.analysis.verdict.notaffected.vulnlog.schema.json"
    },
    "affected": {
      "$ref": "vulnerabilities.analysis.verdict.affected.vulnlog.schema.json"
    }
  }
}
